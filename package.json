{
  "name": "playwright-sample-ts",
  "version": "1.0.0",
  "description": "A sample repository of a Playwright test framework built with Typescript",
  "main": "index.js",
  "scripts": {
    "test": "npx playwright test",
    "test:debug": "npx playwright test --debug",
    "test:api": "npx playwright test --grep api.spec.ts",
    "test:e2e": "npx playwright test --grep e2e.spec.ts",
    "test:ui": "npx playwright test --grep ui.spec.ts"
  },
  "keywords": [],
  "author": "cwf5094",
  "license": "ISC",
  "devDependencies": {
    "@playwright/test": "^1.41.2",
    "@types/mssql": "^9.1.5",
    "@types/node": "^20.11.16",
    "@typescript-eslint/eslint-plugin": "^6.21.0",
    "@typescript-eslint/parser": "^6.21.0",
    "dotenv": "^16.4.1",
    "eslint": "^8.56.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-config-standard-with-typescript": "^43.0.1",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-n": "^16.6.2",
    "eslint-plugin-prettier": "^5.1.3",
    "eslint-plugin-promise": "^6.1.1",
    "prettier": "^3.2.5",
    "typescript": "^5.3.3"
  },
  "dependencies": {
    "mssql": "^10.0.2"
  }
}
